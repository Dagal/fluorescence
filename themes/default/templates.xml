<?xml version="1.0"?>
<templates>

<template name="button">
    <button font="Devinne Swash" font-height="18" height="25">
        <normal font-color="#000000" />
        <mouseover font-color="#000000" />
        <mousedown font-color="#000000" />
    </button>
</template>

<template name="themebutton">
    <button font="Devinne Swash" font-height="18" height="25">
        <normal source="theme" id="images/button.png" font-color="#000000" />
        <mouseover source="theme" id="images/button.png" font-color="#000000" color="#cceecc" />
        <mousedown source="theme" id="images/button.png" font-color="#000000" color="#ccffcc" />
    </button>
</template>

<template name="themebuttonsmall">
    <button font="Devinne Swash" font-height="18" height="25">
        <normal source="theme" id="images/button_small.png" font-color="#000000" />
        <mouseover source="theme" id="images/button_small.png" font-color="#000000" color="#cceecc" />
        <mousedown source="theme" id="images/button_small.png" font-color="#000000" color="#ccffcc" />
    </button>
</template>

<template name="themescrollarea">
    <scrollarea>
        <vscroll width="15" visible="always">
            <thumb>
                <normal source="theme" id="images/scroll_thumb.png" />
                <mouseover source="theme" id="images/scroll_thumb.png" color="#cceecc" />
                <mousedown source="theme" id="images/scroll_thumb.png" color="#ccffcc" />
            </thumb>
            <increment>
                <normal source="theme" id="images/scroll_increment.png" />
                <mouseover source="theme" id="images/scroll_increment.png" color="#cceecc" />
                <mousedown source="theme" id="images/scroll_increment.png" color="#ccffcc" />
            </increment>
            <decrement>
                <normal source="theme" id="images/scroll_decrement.png" />
                <mouseover source="theme" id="images/scroll_decrement.png" color="#cceecc" />
                <mousedown source="theme" id="images/scroll_decrement.png" color="#ccffcc" />
            </decrement>
            <track source="theme" id="images/scroll_track.png" />
        </vscroll>
        <hscroll visible="never" />
    </scrollarea>
</template>

<template name="themelabel" default="1">
    <label font="Devinne Swash" font-height="16" color="#000000" height="20" />
</template>

<template name="themelineedit" default="1">
    <lineedit font="Devinne Swash" font-height="16" color="#000000" height="20" />
</template>

<template name="lineeditbackground">
    <image source="theme" id="images/lineedit_back.png" width="220" height="22" />
</template>

<template name="themecheckbox">
    <checkbox>
        <unchecked source="theme" id="images/checkbox.png" />
        <unchecked-mouseover source="theme" id="images/checkbox.png" color="#cceecc" />
        <checked source="theme" id="images/checkbox_checked.png" />
        <checked-mouseover source="theme" id="images/checkbox_checked.png" color="#cceecc" />
    </checkbox>
</template>

<template name="solid">
    <image source="theme" id="images/solid.png" />
</template>


<template name="greybackground">
    <background x="0" y="0" gumpid="2600"/>
</template>

<template name="blackbackground">
    <background x="0" y="0" gumpid="2620" />
</template>

<template name="thingreybackground">
    <background x="0" y="0" gumpid="3000" />
</template>

<template name="radiobutton">
    <radiobutton>
        <unchecked source="gumpart" id="210" />
        <unchecked-mouseover source="gumpart" id="210" />
        <checked source="gumpart" id="209" />
        <checked-mouseover source="gumpart" id="209" />
    </radiobutton>
</template>

<template name="propertylabel" default="1">
    <propertylabel font="Devinne Swash" font-height="16" align="left" color="#000000" height="20" />
</template>

<template name="sysloglabel" default="1">
    <sysloglabel font="unifont1" font-height="12" align="left" hue="946" />
</template>

<template name="clicklabel" default="1">
    <clicklabel font="unifont1" font-height="12" align="left" color="#000000" />
</template>

<template name="warmodebutton" default="1">
    <warmodebutton>
        <normal source="gumpart" id="2021" />
        <mouseover source="gumpart" id="2023" />
        <mousedown source="gumpart" id="2022" />

        <warmode-normal source="gumpart" id="2024" />
        <warmode-mouseover source="gumpart" id="2026" />
        <warmode-mousedown source="gumpart" id="2025" />
    </warmodebutton>
</template>

<template name="uoscrollarea">
    <scrollarea>
        <vscroll width="15" visible="ondemand">
            <thumb>
                <normal source="gumpart" id="254" />
                <mouseover source="gumpart" id="254" />
                <mousedown source="gumpart" id="254" />
            </thumb>
            <increment>
                <normal source="gumpart" id="253" />
                <mouseover source="gumpart" id="253" />
                <mousedown source="gumpart" id="252" />
            </increment>
            <decrement>
                <normal source="gumpart" id="251" />
                <mouseover source="gumpart" id="251" />
                <mousedown source="gumpart" id="250" />
            </decrement>
            <track source="gumpart" id="255" />
        </vscroll>
        <hscroll visible="never" />
    </scrollarea>
</template>

<template name="blueknob">
    <button>
        <normal source="gumpart" id="2103" />
        <mouseover source="gumpart" id="2103" />
        <mousedown source="gumpart" id="2104" />
    </button>
</template>

<template name="spellbookindex">
    <clicklabel font="Devinne Swash" font-height="16" color="#5a4a31" />
</template>

<template name="spellbooktext">
    <label font="Devinne Swash" font-height="16" color="#5a4a31" />
</template>

<template name="spellbooktitle">
    <label font="Devinne Swash" font-height="16" color="#001052" />
</template>

<template name="htmllabel" default="1">
    <htmllabel scrollbar-template="uoscrollarea" background-template="thingreybackground" font="Devinne Swash" font-height="14" color="#000000" />
</template>

</templates>

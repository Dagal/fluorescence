<?xml version="1.0"?>
<templates>

<template name="background" default="1">
    <background x="0" y="0" gumpid="2600"/>
</template>

<template name="button" font="unifont0" font-size="12" default="1" font-align="center">
    <button>
        <normal source="theme" id="Images/ButtonNormal.png" font-color="#000000" />
        <mouseover source="theme" id="Images/ButtonHot.png" font-color="#000000" />
        <mousedown source="theme" id="Images/ButtonPressed.png" font-color="#000000" />
    </button>
</template>

<template name="checkbox" default="1">
    <checkbox>
        <unchecked source="gumpart" id="210" />
        <unchecked-mouseover source="gumpart" id="210" />
        <checked source="gumpart" id="211" />
        <checked-mouseover source="gumpart" id="211" />
    </checkbox>
</template>

<template name="radiobutton" default="1">
    <radiobutton>
        <unchecked source="gumpart" id="210" />
        <unchecked-mouseover source="gumpart" id="210" />
        <checked source="gumpart" id="209" />
        <checked-mouseover source="gumpart" id="209" />
    </radiobutton>
</template>

<template name="label" default="1">
    <label font="unifont1" font-height="12" align="left" color="#000000" />
</template>

<template name="propertylabel" default="1">
    <propertylabel font="unifont1" font-height="12" align="left" color="#000000" />
</template>

<template name="sysloglabel" default="1">
    <sysloglabel font="unifont1" font-height="12" align="left" hue="946" />
</template>

<template name="clicklabel" default="1">
    <clicklabel font="unifont1" font-height="12" align="left" color="#000000" />
</template>

<template name="lineedit" default="1">
    <lineedit font="unifont1" font-height="12" color="#000000" />
</template>

<template name="warmodebutton" default="1">
    <warmodebutton>
        <normal source="gumpart" id="2021" />
        <mouseover source="gumpart" id="2023" />
        <mousedown source="gumpart" id="2022" />

        <warmode-normal source="gumpart" id="2024" />
        <warmode-mouseover source="gumpart" id="2026" />
        <warmode-mousedown source="gumpart" id="2025" />
    </warmodebutton>
</template>

<template name="scrollarea" default="1">
    <scrollarea>
        <vscroll width="15" visible="ondemand">
            <thumb>
                <normal source="gumpart" id="254" />
                <mouseover source="gumpart" id="254" />
                <mousedown source="gumpart" id="254" />
            </thumb>
            <increment>
                <normal source="gumpart" id="253" />
                <mouseover source="gumpart" id="253" />
                <mousedown source="gumpart" id="252" />
            </increment>
            <decrement>
                <normal source="gumpart" id="251" />
                <mouseover source="gumpart" id="251" />
                <mousedown source="gumpart" id="250" />
            </decrement>
            <track source="gumpart" id="255" />
        </vscroll>
        <hscroll visible="never" />
    </scrollarea>
</template>

</templates>

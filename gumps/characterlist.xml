<gump x="center" y="center" width="250" height="250" closable="0" draggable="0">

    <tscrollarea x="20" y="20" width="210" height="137" hvisible="never" vvisible="always" vstep="28" vpage="112" >
        <repeat name="characterlist" yincrease="28" xlimit="1">
            <tbutton x="0" y="0" width="190" height="25" action="selectcharacter" param="characterindex" text="charactername" />
        </repeat>
    </tscrollarea>
    
    <tbutton x="center" y="175" width="210" height="25" text="Disconnect" action="disconnect" />
    <tbutton x="center" y="203" width="210" height="25" text="Close UO:ME" action="shutdown" />
</gump>
